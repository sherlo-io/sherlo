{
  "name": "@sherlo/react-native-storybook",
  "version": "1.0.19",
  "description": "Sherlo is a Visual Testing tool for React Native Storybook",
  "keywords": [
    "visual testing",
    "react-native",
    "storybook"
  ],
  "license": "MIT",
  "author": "Sherlo",
  "homepage": "https://sherlo.io",
  "bugs": {
    "url": "https://github.com/sherlo-io/sherlo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sherlo-io/sherlo.git",
    "directory": "packages/react-native-storybook"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sherlo": "./cli.js"
  },
  "files": [
    "dist/**/*",
    "android/**/*",
    "ios/**/*",
    "sherlo-react-native-storybook.podspec",
    "cli.js"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p .",
    "dev": "yarn build --watch"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@sherlo/cli": "^1.0.19",
    "base-64": "^0.1.0",
    "jsencrypt": "^3.3.2",
    "node-forge": "^1.3.1",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/node-forge": "^1.3.2",
    "@types/utf8": "^3.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@storybook/react-native": ">=7.6.11",
    "react-native": ">=0.64.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
