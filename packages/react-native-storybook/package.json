{
  "name": "@sherlo/react-native-storybook",
  "version": "1.0.10",
  "description": "Sherlo is a Visual Testing tool for React Native Storybook",
  "keywords": [
    "visual testing",
    "react-native",
    "storybook"
  ],
  "license": "MIT",
  "author": "Sherlo",
  "homepage": "https://sherlo.io",
  "bugs": {
    "url": "https://github.com/sherlo-io/sherlo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sherlo-io/sherlo.git",
    "directory": "packages/react-native-storybook"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "android/**/*",
    "ios/**/*",
    "sherlo-react-native-storybook.podspec"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p .",
    "dev": "yarn build --watch"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "base-64": "0.1.0",
    "jsencrypt": "^3.3.2",
    "node-forge": "^1.3.1",
    "picomatch-browser": "2.2.6",
    "utf8": "3.0.0"
  },
  "devDependencies": {
    "@types/base-64": "1.0.0",
    "@types/node-forge": "^1.3.2",
    "@types/picomatch-browser": "npm:@types/picomatch@2.2.6",
    "@types/utf8": "3.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "*",
    "@storybook/core-events": ">=7.6.15",
    "@storybook/react-native": ">=7.6.15",
    "react": "*",
    "react-native": ">=0.57.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
