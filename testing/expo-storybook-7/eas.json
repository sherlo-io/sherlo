{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "storybook": {
      "env": {
        "EXPO_PUBLIC_STORYBOOK_ONLY": "true"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "env": {
        "EXCLUDE_SHERLO": "true"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
