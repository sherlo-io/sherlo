{
  "name": "@sherlo/expo-storybook-7",
  "version": "1.0.38-alpha",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:android:dev": "eas build --platform android --local --profile=development --output=builds/development/android.apk",
    "build:android:eas": "eas build --platform android --profile=preview",
    "build:android:storybook": "eas build --platform android --local --profile=storybook --output=builds/storybook/android.apk",
    "build:android:preview": "eas build --platform android --local --profile=preview --output=builds/preview/android.apk",
    "build:android:production": "eas build --platform android --local --profile=production --output=builds/production/android.apk",
    "build:ios:dev": "eas build --platform ios --local --profile=development --output=builds/development/ios.tar.gz",
    "build:ios:eas": "eas build --platform ios --profile=preview",
    "build:ios:storybook": "eas build --platform ios --local --profile=storybook --output=builds/storybook/ios.tar.gz",
    "build:ios:preview": "eas build --platform ios --local --profile=preview --output=builds/preview/ios.tar.gz",
    "build:ios:production": "eas build --platform ios --local --profile=production --output=builds/production/ios.tar.gz",
    "eas-build-on-complete": "sherlo eas-build-on-complete --profile=preview",
    "eas-build-post-install": "node ./scripts/setDevEnvOnExpoServer.js",
    "sherlo": "sherlo",
    "sherlo:async": "sherlo --config ../../configs/demo.storybook.json",
    "sherlo:eas": "sherlo --config ../../configs/eas.preview.json",
    "sherlo:sync": "sherlo --config ../../configs/sync.preview.json",
    "start:android:dev": "adb install builds/development/android.apk && expo start --android --dev-client",
    "start:android:go": "expo start --android --go",
    "start:ios:dev": "tar -xzvf builds/development/ios.tar.gz -C builds/ && xcrun simctl install booted builds/sherloexpoexample.app && expo start --ios --dev-client",
    "start:ios:go": "expo start --ios --go",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:web": "storybook dev -p 6006 -c .storybook-web"
  },
  "dependencies": {
    "@expo-google-fonts/urbanist": "^0.2.3",
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-native-community/slider": "4.4.2",
    "@shopify/flash-list": "1.6.3",
    "expo": "~50.0.20",
    "expo-app-loading": "^2.1.1",
    "expo-dev-client": "~3.3.12",
    "expo-font": "~11.10.3",
    "expo-linear-gradient": "~12.7.2",
    "expo-localization": "14.8.4",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "^1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-pager-view": "6.2.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-svg": "^14.1.0",
    "react-native-switch": "^1.5.1",
    "react-native-web": "^0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@sherlo/react-native-storybook": "^1.0.38-alpha",
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-controls": "^7.6.10",
    "@storybook/addon-designs": "^7.0.9",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-ondevice-actions": "^7.6.10",
    "@storybook/addon-ondevice-backgrounds": "^7.6.10",
    "@storybook/addon-ondevice-controls": "^7.6.10",
    "@storybook/addon-ondevice-notes": "^7.6.10",
    "@storybook/addon-react-native-server": "^0.0.5",
    "@storybook/addon-react-native-web": "^0.0.23",
    "@storybook/builder-webpack5": "^7.6.17",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.6.17",
    "@storybook/react-native": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.17",
    "@types/react": "~18.2.14",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.10",
    "eas-cli": "^7.2.0",
    "storybook": "^7.6.17",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2",
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2",
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  },
  "pnpm": {
    "overrides": {
      "react-docgen-typescript": "2.2.2",
      "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
    }
  }
}
