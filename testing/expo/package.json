{
  "name": "@sherlo/expo",
  "version": "1.2.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:android:development": "npx --yes eas-cli build --non-interactive --platform android --local --profile=development --output=builds/development/android.apk",
    "build:android:eas": "npx --yes eas-cli build --non-interactive --platform android --profile=preview",
    "build:android:preview": "npx --yes eas-cli build --non-interactive --platform android --local --profile=preview --output=builds/preview/android.apk",
    "build:ios:development": "npx --yes eas-cli build --non-interactive --platform ios --local --profile=development --output=builds/development/ios.tar.gz",
    "build:ios:eas": "npx --yes eas-cli build --non-interactive --platform ios --profile=preview",
    "build:ios:preview": "npx --yes eas-cli build --non-interactive --platform ios --local --profile=preview --output=builds/preview/ios.tar.gz",
    "clear:devices": "../../scripts/overrideSherloMode.sh --clearAll",
    "update:development": "npx --yes eas-cli update --channel=development --json --non-interactive --message='Update message'",
    "update:preview": "npx --yes eas-cli update --channel=preview --json --non-interactive --message='Update message'",
    "eas-build-on-complete": "npx sherlo eas-build-on-complete --profile=preview",
    "eas-build-post-install": "node ./scripts/setDevEnvOnExpoServer.js",
    "install:android:preview": "adb install builds/preview/android.apk",
    "install:ios:preview": "tar -xzvf builds/preview/ios.tar.gz -C builds/preview && xcrun simctl install booted builds/preview/SherloExpoExample.app",
    "start:android:development": "adb install builds/development/android.apk && npx expo start --android --dev-client",
    "start:android:go": "npx expo start --android --go",
    "start:ios:development": "tar -xzvf builds/development/ios.tar.gz -C builds/development && xcrun simctl install booted builds/development/SherloExpoExample.app && npx expo start --ios --dev-client",
    "start:ios:go": "npx expo start --ios --go",
    "storybook:generate": "sb-rn-get-stories",
    "sherlo": "npx sherlo"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.10.0",
    "@gorhom/bottom-sheet": "^5.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@sherlo/testing-components": "portal:../testing-components",
    "@tamagui/config": "^1.126.12",
    "@tamagui/lucide-icons": "^1.126.12",
    "@tamagui/toast": "^1.126.12",
    "@types/react": "~19.1.10",
    "expo": "^54.0.0",
    "expo-dev-client": "~6.0.15",
    "expo-image": "~3.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-localization": "~17.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-crash-tester": "^0.2.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "15.12.1",
    "react-native-turbo-image": "^1.22.4",
    "react-native-worklets": "0.5.1",
    "tamagui": "^1.126.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/config-plugins": "~54.0.1",
    "@sherlo/react-native-storybook": "portal:../../packages/react-native-storybook",
    "@storybook/addon-ondevice-actions": "^9.1.2",
    "@storybook/addon-ondevice-backgrounds": "^9.1.2",
    "@storybook/addon-ondevice-controls": "^9.1.2",
    "@storybook/addon-ondevice-notes": "^9.1.2",
    "@storybook/react-native": "^9.1.2",
    "@types/lodash": "^4",
    "babel-loader": "^8.2.3",
    "burnt": "^0.13.0",
    "react-native-svg-transformer": "^1.5.2",
    "storybook": "^9.1.2",
    "typescript": "~5.9.2"
  },
  "resolutions": {
    "sherlo": "portal:../../packages/cli"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=4.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
