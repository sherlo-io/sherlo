{
  "cli": {
    "version": ">= 14.5.0"
  },
  "build": {
    "development": {
      "environment": "development",
      "channel": "development",
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "env": {
          "USE_NEW_ARCH": "true"
        }
      },
      "ios": {
        "simulator": true,
        "env": {
          "USE_NEW_ARCH": "true"
        }
      }
    },
    "preview": {
      "environment": "preview",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "env": {
          "USE_NEW_ARCH": "true"
        }
      },
      "ios": {
        "simulator": true,
        "env": {
          "USE_NEW_ARCH": "true"
        }
      }
    }
  }
}
