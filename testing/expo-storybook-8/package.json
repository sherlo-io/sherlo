{
  "name": "@sherlo/expo-storybook-8",
  "version": "1.2.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:android:dev": "npx eas-cli build --platform android --local --profile=development --output=builds/development/android.apk",
    "build:android:eas": "npx eas-cli build --platform android --profile=preview",
    "build:android:preview": "npx eas-cli build --platform android --local --profile=preview --output=builds/preview/android.apk",
    "build:ios:dev": "npx eas-cli build --platform ios --local --profile=development --output=builds/development/ios.tar.gz",
    "build:ios:eas": "npx eas-cli build --platform ios --profile=preview",
    "build:ios:preview": "npx eas-cli build --platform ios --local --profile=preview --output=builds/preview/ios.tar.gz",
    "clear:devices": "../../scripts/overrideSherloMode.sh --clearAll",
    "update:development": "npx eas-cli update --channel=development --json --non-interactive --message='Update message'",
    "update:preview": "npx eas-cli update --channel=preview --json --non-interactive --message='Update message'",
    "eas-build-on-complete": "sherlo eas-build-on-complete --profile=preview",
    "eas-build-post-install": "node ./scripts/setDevEnvOnExpoServer.js",
    "install:android:preview": "adb install builds/preview/android.apk",
    "install:ios:preview": "tar -xzvf builds/preview/ios.tar.gz -C builds/preview && xcrun simctl install booted builds/preview/SherloExpoExample.app",
    "start:android:dev": "adb install builds/development/android.apk && expo start --android --dev-client",
    "start:android:go": "expo start --android --go",
    "start:ios:dev": "tar -xzvf builds/development/ios.tar.gz -C builds/development && xcrun simctl install booted builds/development/SherloExpoExample.app && expo start --ios --dev-client",
    "start:ios:go": "expo start --ios --go",
    "storybook:generate": "sb-rn-get-stories",
    "postinstall": "ln -sf ../testing-components node_modules/@sherlo/testing-components && ln -sf ../../packages/react-native-storybook node_modules/@sherlo/react-native-storybook"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@sherlo/testing-components": "file:../testing-components",
    "@types/react": "~18.3.12",
    "expo": "^52.0.39",
    "expo-dev-client": "^5.0.14",
    "expo-linear-gradient": "~14.0.2",
    "expo-localization": "~16.0.1",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-crash-tester": "^0.2.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@sherlo/react-native-storybook": "file:../../packages/react-native-storybook",
    "@storybook/addon-ondevice-actions": "^8.3.9",
    "@storybook/addon-ondevice-backgrounds": "^8.3.9",
    "@storybook/addon-ondevice-controls": "^8.3.9",
    "@storybook/addon-ondevice-notes": "^8.3.9",
    "@storybook/react-native": "^8.3.9",
    "babel-loader": "^8.2.3",
    "typescript": "~5.3.3"
  }
}
