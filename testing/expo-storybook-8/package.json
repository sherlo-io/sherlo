{
  "name": "@sherlo/expo-storybook-8",
  "version": "1.2.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:android:development:old": "npx eas-cli build --platform android --local --profile=development-old --output=builds/development-old/android.apk",
    "build:android:development:new": "npx eas-cli build --platform android --local --profile=development-new --output=builds/development-new/android.apk",
    "build:android:eas": "npx eas-cli build --platform android --profile=preview",
    "build:android:preview:old": "npx eas-cli build --platform android --local --profile=preview-old --output=builds/preview-old/android.apk",
    "build:android:preview:new": "npx eas-cli build --platform android --local --profile=preview-new --output=builds/preview-new/android.apk",
    "build:ios:development:old": "npx eas-cli build --platform ios --local --profile=development-old --output=builds/development-old/ios.tar.gz",
    "build:ios:development:new": "npx eas-cli build --platform ios --local --profile=development-new --output=builds/development-new/ios.tar.gz",
    "build:ios:eas": "npx eas-cli build --platform ios --profile=preview",
    "build:ios:preview:old": "npx eas-cli build --platform ios --local --profile=preview-old --output=builds/preview-old/ios.tar.gz",
    "build:ios:preview:new": "npx eas-cli build --platform ios --local --profile=preview-new --output=builds/preview-new/ios.tar.gz",
    "clear:devices": "../../scripts/overrideSherloMode.sh --clearAll",
    "update:development": "npx eas-cli update --channel=development --json --non-interactive --message='Update message'",
    "update:preview": "npx eas-cli update --channel=preview --json --non-interactive --message='Update message'",
    "eas-build-on-complete": "npx sherlo eas-build-on-complete --profile=preview",
    "eas-build-post-install": "node ./scripts/setDevEnvOnExpoServer.js",
    "install:android:preview:old": "adb install builds/preview-old/android.apk",
    "install:android:preview:new": "adb install builds/preview-new/android.apk",
    "install:ios:preview:old": "tar -xzvf builds/preview-old/ios.tar.gz -C builds/preview-old && xcrun simctl install booted builds/preview-old/SherloExpoExample.app",
    "install:ios:preview:new": "tar -xzvf builds/preview-new/ios.tar.gz -C builds/preview-new && xcrun simctl install booted builds/preview-new/SherloExpoExample.app",
    "start:android:development:old": "adb install builds/development-old/android.apk && expo start --android --dev-client",
    "start:android:development:new": "adb install builds/development-new/android.apk && expo start --android --dev-client",
    "start:android:go": "expo start --android --go",
    "start:ios:development:old": "tar -xzvf builds/development-old/ios.tar.gz -C builds/development-old && xcrun simctl install booted builds/development-old/SherloExpoExample.app && expo start --ios --dev-client",
    "start:ios:development:new": "tar -xzvf builds/development-new/ios.tar.gz -C builds/development-new && xcrun simctl install booted builds/development-new/SherloExpoExample.app && expo start --ios --dev-client",
    "start:ios:go": "expo start --ios --go",
    "storybook:generate": "sb-rn-get-stories",
    "sherlo": "npx sherlo"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@sherlo/testing-components": "portal:../testing-components",
    "@types/react": "~18.3.12",
    "expo": "^52.0.46",
    "expo-dev-client": "~5.0.20",
    "expo-linear-gradient": "~14.0.2",
    "expo-localization": "~16.0.1",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.4",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-crash-tester": "^0.2.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@sherlo/react-native-storybook": "portal:../../packages/react-native-storybook",
    "@storybook/addon-ondevice-actions": "^8.3.9",
    "@storybook/addon-ondevice-backgrounds": "^8.3.9",
    "@storybook/addon-ondevice-controls": "^8.3.9",
    "@storybook/addon-ondevice-notes": "^8.3.9",
    "@storybook/react-native": "^8.3.9",
    "@types/lodash": "^4",
    "babel-loader": "^8.2.3",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "sherlo": "portal:../../packages/cli"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=4.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
