{
  "name": "@sherlo/expo-storybook-8",
  "version": "1.2.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:android:development:old": "npx eas-cli build --non-interactive --platform android --local --profile=development-old --output=builds/development-old/android.apk",
    "build:android:development:new": "npx eas-cli build --non-interactive --platform android --local --profile=development-new --output=builds/development-new/android.apk",
    "build:android:eas": "npx eas-cli build --non-interactive --platform android --profile=preview",
    "build:android:preview:old": "npx eas-cli build --non-interactive --platform android --local --profile=preview-old --output=builds/preview-old/android.apk",
    "build:android:preview:new": "npx eas-cli build --non-interactive --platform android --local --profile=preview-new --output=builds/preview-new/android.apk",
    "build:ios:development:old": "npx eas-cli build --non-interactive --platform ios --local --profile=development-old --output=builds/development-old/ios.tar.gz",
    "build:ios:development:new": "npx eas-cli build --non-interactive --platform ios --local --profile=development-new --output=builds/development-new/ios.tar.gz",
    "build:ios:eas": "npx eas-cli build --non-interactive --platform ios --profile=preview",
    "build:ios:preview:old": "npx eas-cli build --non-interactive --platform ios --local --profile=preview-old --output=builds/preview-old/ios.tar.gz",
    "build:ios:preview:new": "npx eas-cli build --non-interactive --platform ios --local --profile=preview-new --output=builds/preview-new/ios.tar.gz",
    "clear:devices": "../../scripts/overrideSherloMode.sh --clearAll",
    "update:development": "npx eas-cli update --channel=development --json --non-interactive --message='Update message'",
    "update:preview": "npx eas-cli update --channel=preview --json --non-interactive --message='Update message'",
    "eas-build-on-complete": "npx sherlo eas-build-on-complete --profile=preview-new,preview-old",
    "eas-build-post-install": "node ./scripts/setDevEnvOnExpoServer.js",
    "install:android:preview:old": "adb install builds/preview-old/android.apk",
    "install:android:preview:new": "adb install builds/preview-new/android.apk",
    "install:ios:preview:old": "tar -xzvf builds/preview-old/ios.tar.gz -C builds/preview-old && xcrun simctl install booted builds/preview-old/SherloExpoExample.app",
    "install:ios:preview:new": "tar -xzvf builds/preview-new/ios.tar.gz -C builds/preview-new && xcrun simctl install booted builds/preview-new/SherloExpoExample.app",
    "start:android:development:old": "adb install builds/development-old/android.apk && npx expo start --android --dev-client",
    "start:android:development:new": "adb install builds/development-new/android.apk && npx expo start --android --dev-client",
    "start:android:go": "npx expo start --android --go",
    "start:ios:development:old": "tar -xzvf builds/development-old/ios.tar.gz -C builds/development-old && xcrun simctl install booted builds/development-old/SherloExpoExample.app && npx expo start --ios --dev-client",
    "start:ios:development:new": "tar -xzvf builds/development-new/ios.tar.gz -C builds/development-new && xcrun simctl install booted builds/development-new/SherloExpoExample.app && npx expo start --ios --dev-client",
    "start:ios:go": "npx expo start --ios --go",
    "storybook:generate": "sb-rn-get-stories",
    "sherlo": "npx sherlo"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.10.0",
    "@gorhom/bottom-sheet": "^5.0.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@sherlo/testing-components": "portal:../testing-components",
    "@tamagui/config": "^1.126.12",
    "@tamagui/lucide-icons": "^1.126.12",
    "@tamagui/toast": "^1.126.12",
    "@types/react": "~19.0.10",
    "expo": "^53.0.20",
    "expo-dev-client": "~5.2.4",
    "expo-image": "^2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-localization": "~16.1.6",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-crash-tester": "^0.2.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-svg": "15.11.2",
    "react-native-turbo-image": "^1.22.4",
    "tamagui": "^1.126.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/config-plugins": "^8.0.0",
    "@sherlo/react-native-storybook": "portal:../../packages/react-native-storybook",
    "@storybook/addon-ondevice-actions": "^9.1.2",
    "@storybook/addon-ondevice-backgrounds": "^9.1.2",
    "@storybook/addon-ondevice-controls": "^9.1.2",
    "@storybook/addon-ondevice-notes": "^9.1.2",
    "@storybook/react-native": "^9.1.2",
    "@types/lodash": "^4",
    "babel-loader": "^8.2.3",
    "burnt": "^0.13.0",
    "eas-cli": "16.3.2",
    "storybook": "^9.1.2",
    "typescript": "~5.8.3"
  },
  "resolutions": {
    "sherlo": "portal:../../packages/cli"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=4.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
